generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

model ClaimGaransi {
    id           Int      @id @default(autoincrement())
    serialnumber String
    tipe         String
    part         String
    createdAt    DateTime @default(now())
    updatedAt    DateTime
}

model Customer {
    id        Int      @id @default(autoincrement())
    name      String
    no_telp   String
    alamat    String
    createdAt DateTime @default(now())
    updatedAt DateTime
}

model Penawaran {
    id           Int      @id @default(autoincrement())
    serialnumber String
    tipe         String
    part         String
    createdAt    DateTime @default(now())
    updatedAt    DateTime
}

model ServiceIn {
    id           Int      @id @default(autoincrement())
    serialnumber String
    tipe         String
    kelengkapan  String
    createdAt    DateTime @default(now())
    updatedAt    DateTime
}

model ServiceOut {
    id           Int      @id @default(autoincrement())
    serialnumber String
    tipe         String
    kelengkapan  String
    createdAt    DateTime @default(now())
    updatedAt    DateTime
}

model User {
    id        Int      @id @default(autoincrement())
    name      String
    username  String
    password  String
    createdAt DateTime @default(now())
    updatedAt DateTime
}
